{% extends 'base_usuario_industria.html.twig' %}
{% block stylesheets %}
    {{parent()}}
    {{ encore_entry_link_tags('select2') }}  
    {{ encore_entry_link_tags('datepicker') }}
{% endblock %}
{% block body %}
    <div class="container w-100 p-3">
        <div class="card mx-auto">
            <div class="card-header bg-info" >
                <div class="row">
                    <div class="col-6">
                        <h4 class="text-light mb-3 font-weight-normal">Nueva Industria</h4> 
                    </div>
                    <div class="col-6">

                        <div class="float-right">    
                            <div class="center-block text-center">
                                <a class="btn btn-light" href="{{ path("industria_logout") }}">Salir</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body">  
                {{ include('industria/_formulario.html.twig') }}
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('select2') }}  
    {{ encore_entry_script_tags('datepicker') }}  
    {{ encore_entry_script_tags('lugarForm') }}
{% endblock %}