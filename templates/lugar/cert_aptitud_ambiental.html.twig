<h4 class="card-title">Certificado de aptitud ambiental</h4>
<h5 class="card-title">Dec. 531/19 y su modificatorio 973/20 (Reglamento de la ley 11459/93)</h5>
<div class="row">
    <div class="col-12">
        <div class="btn-toolbar" role="toolbar">
            <div class="pr-4 pt-2">
                ¿Tiene Disposición de Categorización Provincial?
            </div>
            <div class="btn-group mr-2" role="group">
                <button id="btnDisposicionSi" type="button" class="btn btn-primary">Si</button>
            </div>
            <div class="btn-group mr-2" role="group">            
                <button id="btnDisposicionNo" type="button" class="btn btn-primary">No</button>
            </div>
        </div> 
    </div>  
</div>
<br/>
<div class="row" id="divDisposicion">
    <div class="col-12">
        <div class="card">
            <div class="card-body">               
                <div class="row">
                    <div class="col-6">
                        <div class="form-group">
                            {{ form_label(dispocisionProvincial) }}
                            {{ form_widget(dispocisionProvincial) }}
                        </div> 
                    </div>
                    <div class="col-6">
                        <div class="form-group">
                            {{ form_label(fechaOtorgDispProv) }}
                            {{ form_widget(fechaOtorgDispProv) }}
                        </div> 
                    </div>
                </div>
                <div class="row">
                    <div class="col-6">
                        <div class="form-group">
                            {{ form_label(certAptitudAmbiental.categoria) }}
                            {{ form_widget(certAptitudAmbiental.categoria) }}
                        </div>
                    </div> 
                </div>
            </div>
        </div>
    </div>
</div>
<br/>
<div class="row">
    <div class="col-12">
        <div class="btn-toolbar" role="toolbar">
            <div class="pr-4 pt-2">
                ¿Tiene Certificado de Aptitud Ambiental?
            </div>
            <div class="btn-group mr-2" role="group">
                <button id="btnCertSi" type="button" class="btn btn-primary">Si</button>
            </div>
            <div class="btn-group mr-2" role="group">            
                <button id="btnCertNo" type="button" class="btn btn-primary">No</button>
            </div>
        </div> 
    </div>  
</div>
<br/> 
<div class="row" id="divCertAptitudAmbiental">
    <div class="col-12">
        <div class="card">
            <div class="card-body">     
                <div class="row">
                    <div class="col-6">
                        <div class="form-group">
                            {{ form_label(certAptitudAmbiental.numero) }}
                            {{ form_widget(certAptitudAmbiental.numero) }}
                        </div>
                    </div> 
                </div>
                <div class="row">                    
                    <div class="col-6">
                        <div class="form-group">
                            {{ form_label(certAptitudAmbiental.fechaOtorgamiento) }}
                            {{ form_widget(certAptitudAmbiental.fechaOtorgamiento) }}
                        </div> 
                    </div>
                    <div class="col-6">
                        <div class="form-group">
                            {{ form_label(certAptitudAmbiental.fechaVencimiento) }}
                            {{ form_widget(certAptitudAmbiental.fechaVencimiento) }}
                        </div> 
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>