{% extends 'base.html.twig' %}
{% block title %}Editar prototipo{% endblock %}
{% block body %}
    <div class="container w-100 p-3">
        <div class="card mx-auto">
            <div class="card-header bg-info" >
                <h4 class="text-light mb-3 font-weight-normal">Editar prototipo</h4>     
            </div>
            <div class="card-body">  
                {{ include('Prototipo/_prototipo_form.html.twig') }}
                <br>
                <div class="float-right">
                    <a class="btn btn-secondary pr-2 " href="{{ path("prototipo") }}">Volver a la lista</a>
                    <button class="btn btn-primary ">{{ button_label|default('Editar') }}</button>    
                </div>    
            </div>
        </div>

    </div>
{% endblock %}